{
        "name": "test-network-org1",
        "version": "1.0",
        "client": {
            "organization": "carrier-net",
            "connection": {
                "timeout": {
                    "peer": {
                        "endorser": "300"
                    }
                }
            }
        },
        "organizations": {
            "carrier-net": {
                "mspid": "carrierMSP",
                "peers": [
                    "peer0"
                ],
                "certificateAuthorities": [
                    "ca"
                ]
            }
        },
        "peers": {
            "peer0": {
                "url": "grpcs://peer0.carrier-net.svc.local:7051",
                "tlsCACerts": {
                    "path": "secret/carrier/peer/msp/tlscacerts/tlsca.pem"
                },
                "grpcOptions": {
                    "ssl-target-name-override": "peer0",
                    "hostnameOverride": "peer0"
                }
            }
        },
        "certificateAuthorities": {
            "ca": {
                "url": "https://ca.carrier-net.svc.local:7054",
                "caName": "ca.carrier-net",
                "tlsCACerts": {
                    "path": ["secret/endorsers/carrier/msp/cacerts/ca.pem"]
                },
                "httpOptions": {
                    "verify": false
                }
            }
        }
    }
    