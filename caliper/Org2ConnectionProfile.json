{
        "name": "test-network-org1",
        "version": "1.0",
        "client": {
            "organization": "manufacturer-net",
            "connection": {
                "timeout": {
                    "peer": {
                        "endorser": "3000"
                    }
                }
            }
        },
        "organizations": {
            "manufacturer-net": {
                "mspid": "manufacturerMSP",
                "peers": [
                    "peer0"
                ],
                "certificateAuthorities": [
                    "ca"
                ]
            }
        },
        "peers": {
            "peer0": {
                "url": "grpcs://peer0.manufacturer-net:7051",
                "tlsCACerts": {
                    "path": "secret/manufacturer/peer/msp/tlscacerts/tlsca.pem"
                },
                "grpcOptions": {
                    "ssl-target-name-override": "peer0",
                    "hostnameOverride": "peer0"
                }
            }
        },
        "certificateAuthorities": {
            "ca": {
                "url": "https://ca.manufacturer-net:7054",
                "caName": "ca.manufacturer-net",
                "tlsCACerts": {
                    "path": ["secret/endorsers/manufacturer/msp/cacerts/ca.pem"]
                },
                "httpOptions": {
                    "verify": false
                }
            }
        }
    }
    